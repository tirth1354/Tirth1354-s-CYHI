
#include<stdio.h>
int main()
{
    int n;
    n=5;
    int array[n][n];
    for (int i=0;i<n;i++)
    {
        printf("Enter Quiz-%d grade: ",i+1); 
        for(int j=0;j<n;j++) 
        {
            scanf("%d",&array[i][j]);
        }
    }
    int row_total[n];
    int column_total[n];
    int row_sum=0,column_sum=0;
    float avg_std[n];
    float avg_quiz[n];
    int high_score,low_score=0;
    int high_quiz[n];
    int low_quiz[n];
    for (int i=0;i<n;i++)
    {
        high_score = array[i][0];
        low_score = array[i][0];
        for(int j=0;j<n;j++) 
        {
            row_sum += array[i][j]; 
            high_score = high_score>array[i][j] ? high_score:array[i][j];
            low_score = low_score<array[i][j] ? low_score:array[i][j];
            column_sum += array[j][i];
        }
        row_total[i] = row_sum;
        column_total[i] = column_sum;
        avg_quiz[i] = (float)row_sum/n;
        avg_std[i] = (float)column_sum/n;
        high_quiz[i] = high_score;
        low_quiz[i] = low_score;
        row_sum = 0;
        column_sum = 0;
    }
    for (int i=0;i<n;i++)
    {
        printf("\nTotal score of student %d: %d",i+1,column_total[i]);
        printf("\nAverage score of student %d:%.2f\n",i+1,avg_std[i]);
    }
    for(int i=0;i<n;i++)
    {
        printf("\nAverage score in Quiz %d: %.2f",i+1,avg_quiz[i]);
        printf("\nHigh score in Quiz %d: %d",i+1,high_quiz[i]);
        printf("\nLow score in Quiz %d: %d\n",i+1,low_quiz[i]);
    }
    return 0;
}
